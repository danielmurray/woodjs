{
  "name": "woodjs",
  "description": "M@terial for M@arionette... Wood",
  "repository": {
    "type": "git",
  },
  "publishConfig": {
  },
  "version": "0.0.0",
  "scripts": {
    "watch-sass":  "node-sass -wr src/sass/all.scss dist/css/wood.css",
    "watch-js": "watchify src/js/all.js -o dist/js/wood.js -dv",
    "watch": "npm run watch-sass & npm run watch-js",
    "build-sass":  "node-sass --output-style compressed src/sass/all.scss dist/css/wood.min.css",
    "build-js": "browserify src/js/all.js | uglifyjs -mc > dist/js/wood.min.js",
    "build": "npm run build-sass & npm run build-js"
  },
  "author": "Dan Murray",
  "license": "ISC",
  "devDependencies": {
    "browserify": "",
    "express": "^4.13.0",
    "uglify-js": "",
    "watchify": "",
    "node-sass": ""
  }
}
